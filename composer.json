{
  "name": "radishlab/myc-getter",
  "description": "A library to get and escape values in WordPress + ACF environments",
  "version": "1.0.4",
  "license": "GPL-2.0-or-later",
  "autoload": {
    "psr-4": {
      "RadishLab\\MycGetter\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Radish Lab",
      "homepage": "https://radishlab.com/"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
